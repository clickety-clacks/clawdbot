{
  "gateway": {
    "mode": "local",
    "bind": "loopback"
  },
  "agents": {
    "defaults": {
      "workspace": "~/clawdbot-workspace",
      "model": {
        "primary": "anthropic/claude-3-sonnet"
      }
    }
  },
  "clawline": {
    "port": 19992,
    "network": {
      "bindAddress": "127.0.0.1",
      "allowInsecurePublic": true,
      "allowedOrigins": ["null"]
    }
  }
}
